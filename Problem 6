#include <conio.h>
#include <iostream>

using namespace std;

int main ()

{
	int x, next, n1, n2;
	n1 = 0;
	n2 = 1;
	cout << "Fibonacci numbers: "<< endl;
	
for (x = 0; x <= 21; x++)
    {
        if(x == 0)
        {
            cout << n1 << ", ";
            continue;
        }
        if(x == 1)
        {
            cout << n2 << ", ";
            continue;
        }
        next = n1 + n2;
        n1 = n2;
        n2 = next;
        
        cout <<  next << ", ";
	}
	return 0;
}
